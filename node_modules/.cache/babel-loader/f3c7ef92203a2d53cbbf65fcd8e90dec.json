{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"TransferCreate\",\n  data() {\n    return {\n      errors: [],\n      loading: [],\n      model: []\n    };\n  },\n  computed: {\n    inputClass() {\n      return ['form-control', this.errors.length ? 'error' : null].join(' ');\n    },\n    selectClass() {\n      return ['form-control', 'form-select', this.errors.length ? 'error' : null].join(' ');\n    }\n  },\n  methods: {\n    getCategoryBalanceAnalyticsData() {\n      this.setLoading(['categoryBalanceAnalyticsData']);\n      this.axios.defaults.headers.common['Authorization'] = `Bearer ${localStorage.getItem('bearer_token')}`;\n      this.axios.defaults.headers.common['Content-Type'] = 'application/json';\n      this.axios.defaults.headers.common['Accept'] = 'application/json';\n      this.axios.get('http://is.test/api/analytics/out-category-balance').catch(error => {\n        if (error.response.status === 401) {\n          localStorage.removeItem('bearer_token');\n          window.location.href = '/';\n        }\n      }).then(response => {\n        if (response.status === 200) {\n          this.categoryBalanceAnalyticsData = response.data.data;\n        }\n      }).finally(() => {\n        this.removeLoading(['categoryBalanceAnalyticsData']);\n      });\n    },\n    getCategories() {\n      this.setLoading(['category']);\n      this.axios.defaults.headers.common['Authorization'] = `Bearer ${localStorage.getItem('bearer_token')}`;\n      this.axios.defaults.headers.common['Content-Type'] = 'application/json';\n      this.axios.defaults.headers.common['Accept'] = 'application/json';\n      this.axios.get('http://is.test/api/categories').catch(error => {\n        if (error.response.status === 401) {\n          localStorage.removeItem('bearer_token');\n          window.location.href = '/';\n        }\n      }).then(response => {\n        if (response.status === 200) {\n          this.categories = response.data.data.filter(item => item.type === 'expenses');\n        }\n      }).finally(() => {\n        this.removeLoading(['category']);\n      });\n    },\n    getAccounts() {\n      this.setLoading(['account']);\n      this.axios.defaults.headers.common['Authorization'] = `Bearer ${localStorage.getItem('bearer_token')}`;\n      this.axios.defaults.headers.common['Content-Type'] = 'application/json';\n      this.axios.defaults.headers.common['Accept'] = 'application/json';\n      this.axios.get('http://is.test/api/accounts').catch(error => {\n        if (error.response.status === 401) {\n          localStorage.removeItem('bearer_token');\n          window.location.href = '/';\n        }\n      }).then(response => {\n        if (response.status === 200) {\n          this.accounts = response.data.data.filter(item => item.user_id === 1);\n        }\n      }).finally(() => {\n        this.removeLoading(['account']);\n      });\n    },\n    isLoading(key) {\n      return this.loading.includes(key);\n    },\n    setLoading(keys) {\n      keys.forEach(key => {\n        this.loading.push(key);\n      });\n      return this.loading;\n    },\n    removeLoading(keys) {\n      keys.forEach(key => {\n        for (var i = 0; i < this.loading.length; i++) {\n          if (this.loading[i] === key) {\n            this.loading.splice(i, 1);\n          }\n        }\n      });\n      return this.loading;\n    },\n    submit() {\n      this.setLoading(['submit']);\n      this.axios.post('http://is.test/api/transfers', this.model).catch(error => {\n        if (error.response.status === 401) {\n\n          // this.errors =\n        }\n        if (error.response.status === 422) {\n          console.log(1, error.response.errors.errors);\n          // this.errors =\n        }\n      }).then(response => {\n        console.log(2, response.status);\n      }).finally(() => {\n        this.removeLoading(['submit']);\n      });\n    }\n  },\n  created() {\n    this.getCategoryBalanceAnalyticsData();\n    this.getCategories();\n    this.getAccounts();\n  }\n};","map":{"version":3,"mappings":";AAiDA,eAAe;EACbA,IAAI,EAAE,gBAAgB;EACtBC,IAAI,GAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE;IACT;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAU,GAAG;MACX,OAAO,CAAC,cAAc,EAAE,IAAI,CAACJ,MAAM,CAACK,MAAK,GAAI,OAAM,GAAI,IAAI,CAAC,CAACC,IAAI,CAAC,GAAG;IACvE,CAAC;IACDC,WAAW,GAAG;MACZ,OAAO,CAAC,cAAc,EAAE,aAAa,EAAE,IAAI,CAACP,MAAM,CAACK,MAAK,GAAI,OAAM,GAAI,IAAI,CAAC,CAACC,IAAI,CAAC,GAAG;IACtF;EACF,CAAC;EACDE,OAAO,EAAE;IACPC,+BAA+B,GAAG;MAChC,IAAI,CAACC,UAAU,CAAC,CAAC,8BAA8B,CAAC;MAEhD,IAAI,CAACC,KAAK,CAACC,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,eAAe,IAAK,UAASC,YAAY,CAACC,OAAO,CAAC,cAAc,CAAE,EAAC;MACtG,IAAI,CAACL,KAAK,CAACC,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,cAAc,IAAI,kBAAiB;MACtE,IAAI,CAACH,KAAK,CAACC,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,QAAQ,IAAI,kBAAiB;MAEhE,IAAI,CAACH,KAAK,CAACM,GAAG,CAAC,mDAAmD,EAC7DC,KAAK,CAAEC,KAAK,IAAK;QAChB,IAAIA,KAAK,CAACC,QAAQ,CAACC,MAAK,KAAM,GAAG,EAAE;UACjCN,YAAY,CAACO,UAAU,CAAC,cAAc;UACtCC,MAAM,CAACC,QAAQ,CAACC,IAAG,GAAI,GAAE;QAC3B;MACF,CAAC,EACAC,IAAI,CAAEN,QAAQ,IAAK;QAClB,IAAIA,QAAQ,CAACC,MAAK,KAAM,GAAG,EAAE;UAC3B,IAAI,CAACM,4BAA2B,GAAIP,QAAQ,CAACrB,IAAI,CAACA,IAAG;QACvD;MACF,CAAC,EACA6B,OAAO,CAAC,MAAM;QACb,IAAI,CAACC,aAAa,CAAC,CAAC,8BAA8B,CAAC;MACrD,CAAC;IAEP,CAAC;IACDC,aAAa,GAAG;MACd,IAAI,CAACpB,UAAU,CAAC,CAAC,UAAU,CAAC;MAE5B,IAAI,CAACC,KAAK,CAACC,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,eAAe,IAAK,UAASC,YAAY,CAACC,OAAO,CAAC,cAAc,CAAE,EAAC;MACtG,IAAI,CAACL,KAAK,CAACC,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,cAAc,IAAI,kBAAiB;MACtE,IAAI,CAACH,KAAK,CAACC,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,QAAQ,IAAI,kBAAiB;MAEhE,IAAI,CAACH,KAAK,CAACM,GAAG,CAAC,+BAA+B,EACzCC,KAAK,CAAEC,KAAK,IAAK;QAChB,IAAIA,KAAK,CAACC,QAAQ,CAACC,MAAK,KAAM,GAAG,EAAE;UACjCN,YAAY,CAACO,UAAU,CAAC,cAAc;UACtCC,MAAM,CAACC,QAAQ,CAACC,IAAG,GAAI,GAAE;QAC3B;MACF,CAAC,EACAC,IAAI,CAAEN,QAAQ,IAAK;QAClB,IAAIA,QAAQ,CAACC,MAAK,KAAM,GAAG,EAAE;UAC3B,IAAI,CAACU,UAAS,GAAIX,QAAQ,CAACrB,IAAI,CAACA,IAAI,CAACiC,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACC,IAAG